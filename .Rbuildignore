^MultiNMix\.Rproj$
^\.Rproj\.user$           # Exclude RStudio's user-specific settings
^LICENSE\.md$             # Exclude Markdown version of the LICENSE file
^README\.md$              # Exclude README Markdown file (if not using as a vignette)
^README\.Rmd$             # Exclude README RMarkdown file (if not rendered as .md)
^\.git$                   # Exclude Git version control folder
^\.gitignore$             # Exclude Git ignore file
^\.DS_Store$              # Exclude macOS-specific hidden files
^vignettes/.*\.html$      # Exclude precompiled HTML vignettes
^vignettes/.*\.R$         # Exclude intermediate vignette files
^vignettes/.*\.log$       # Exclude log files generated during vignette building
^inst/doc/.*              # Exclude prebuilt documentation in `inst/doc`
^man/.*\.Rd~$             # Exclude temporary backup Rd files
^.*\.log$                 # Exclude general log files
^.*\.aux$                 # Exclude auxiliary files generated by LaTeX
^.*\.toc$                 # Exclude table of contents files (LaTeX)
^tests/testthat\.Rout$    # Exclude test output files
^tests/testthat\.Rout\.fail$ # Exclude failed test output files
^LICENSE\.md$
